    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rust_debug_test'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rust_debug_test",
                    "--package=rust_debug_test"
                ],
                "filter": {
                    "name": "rust_debug_test",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch rust_debug_test",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/rust_debug_test",
            "args": [],
        },
    ]
